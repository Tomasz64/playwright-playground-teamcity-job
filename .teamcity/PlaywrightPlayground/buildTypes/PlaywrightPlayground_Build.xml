  <name>Build</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="playwright-report =&gt; playwright-report" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="Install Dependencies" type="nodejs-runner">
