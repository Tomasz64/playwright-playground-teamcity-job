      <runner id="RUNNER_2" name="Run Tests" type="nodejs-runner">
        <parameters>
          <param name="plugin.docker.imageId" value="mcr.microsoft.com/playwright" />
          <param name="shellScript"><![CDATA[npx playwright install
npx playwright test]]></param>
